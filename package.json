{
  "name": "hills-api",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "audit": "npm-run-all --continue-on-error --parallel audit:*",
    "audit:legal": "license-checker --summary --production --excludePrivatePackages --failOn UNLICENSED",
    "audit:security": "npm audit",
    "call": "bash scripts/call.sh",
    "call:local": "bash scripts/call.sh 'http://localhost:3000/graphql'",
    "check": "npm-run-all --continue-on-error --parallel check:*",
    "check:lint": "eslint ./**/*.js",
    "check:prettier": "prettier --check **/*.{graphql,js,json,md,yml}",
    "deploy:all": "npm-run-all \"deploy:db:all -- {@}\" \"deploy:stack -- {@}\" --",
    "deploy:db:all": "npm-run-all \"deploy:db:cluster -- {@}\" \"deploy:db:tables -- {@}\" \"deploy:db:data -- {@}\" --",
    "deploy:db:cluster": "serverless deploy --config ./database/serverless-db.yml",
    "deploy:db:data": "serverless deploy-data --config ./database/serverless-db.yml",
    "deploy:db:tables": "serverless deploy-tables --config ./database/serverless-db.yml",
    "deploy:stack": "serverless deploy",
    "deploy:update-fn": "serverless deploy function --function",
    "deployment:info": "serverless info",
    "package": "serverless package",
    "start": "serverless offline --printOutput",
    "test:integration": "jest --testEnvironment=node ./src/integration",
    "undeploy:all": "npm-run-all \"undeploy:db:all -- {@}\" \"undeploy:stack -- {@}\" --",
    "undeploy:db:all": "serverless remove --config ./database/serverless-db.yml",
    "undeploy:stack": "serverless remove"
  },
  "dependencies": {
    "aws-sdk": "^2.529.0",
    "aws-serverless-express": "^3.3.6",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.4",
    "graphql-playground-middleware-lambda": "^1.7.13",
    "graphql-resolve-batch": "^1.0.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "csv-parse": "^4.6.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-unicorn": "^12.0.2",
    "jest": "^24.9.0",
    "license-checker": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "serverless": "^1.51.0",
    "serverless-offline": "^5.10.1",
    "serverless-plugin-git-variables": "^3.3.2",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-prune-plugin": "^1.4.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
