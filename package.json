{
  "name": "hills-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "check": "npm-run-all --continue-on-error --parallel check:**",
    "check:lint": "eslint src/**/*.js",
    "check:prettier": "prettier --ignore-path=./.gitignore --check **/*.{graphql,js,json,md,yml}",
    "deploy:data": "node ./scripts/hillsDB/populateDynamoDB.js",
    "deploy:sls": "serverless deploy",
    "package": "serverless package",
    "start": "serverless offline --printOutput",
    "undeploy": "serverless remove"
  },
  "devDependencies": {
    "aws-sdk": "^2.529.0",
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "serverless": "^1.51.0",
    "serverless-offline": "^5.10.1"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.9.2",
    "csv-parse": "^4.4.5",
    "graphql": "^14.5.4",
    "lodash.merge": "^4.6.2"
  }
}
