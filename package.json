{
  "name": "hills-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "check": "npm-run-all --continue-on-error --parallel check:**",
    "check:lint": "eslint src/**/*.js",
    "check:prettier": "prettier --check **/*.{graphql,js,json,md,yml}",
    "deploy:data": "node ./scripts/hillsDB/populateDynamoDB.js",
    "deploy:stack": "serverless deploy",
    "deploy:update-fn": "serverless deploy function --function graphql",
    "deployment:info": "serverless info",
    "package": "serverless package",
    "smoke-test": "sh scripts/smokeTest.sh",
    "start": "serverless offline --printOutput",
    "undeploy": "serverless remove"
  },
  "devDependencies": {
    "csv-parse": "^4.4.5",
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "serverless": "^1.51.0",
    "serverless-offline": "^5.10.1",
    "serverless-plugin-git-variables": "^3.3.2",
    "serverless-prune-plugin": "^1.4.1"
  },
  "dependencies": {
    "aws-sdk": "^2.529.0",
    "aws-serverless-express": "^3.3.6",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "lodash.merge": "^4.6.2"
  }
}
