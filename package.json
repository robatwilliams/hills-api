{
  "name": "hills-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "call": "bash scripts/call.sh",
    "call:local": "bash scripts/call.sh 'http://localhost:3000/graphql'",
    "check": "npm-run-all --continue-on-error --parallel check:**",
    "check:lint": "eslint ./**/*.js",
    "check:prettier": "prettier --check **/*.{graphql,js,json,md,yml}",
    "deploy:all": "npm-run-all \"deploy:database -- {@}\" \"deploy:data -- {@}\" \"deploy:stack -- {@}\" --",
    "deploy:data": "serverless deploy-data --config serverless-db.yml",
    "deploy:database": "serverless deploy --config serverless-db.yml",
    "deploy:stack": "serverless deploy",
    "deploy:update-fn": "serverless deploy function --function",
    "deployment:info": "serverless info",
    "package": "serverless package",
    "start": "serverless offline --printOutput",
    "undeploy:all": "npm-run-all \"undeploy:database -- {@}\" \"undeploy:stack -- {@}\" --",
    "undeploy:stack": "serverless remove",
    "undeploy:database": "serverless remove --config serverless-db.yml"
  },
  "devDependencies": {
    "csv-parse": "^4.4.5",
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "serverless": "^1.51.0",
    "serverless-offline": "^5.10.1",
    "serverless-plugin-git-variables": "^3.3.2",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-prune-plugin": "^1.4.1"
  },
  "dependencies": {
    "aws-sdk": "^2.529.0",
    "aws-serverless-express": "^3.3.6",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.4",
    "graphql-playground-middleware-lambda": "^1.7.13",
    "graphql-resolve-batch": "^1.0.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "lodash.merge": "^4.6.2"
  }
}
